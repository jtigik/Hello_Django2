{
    "builds": [
        {
            "src": "HelloProject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "HelloProject/wsgi.py"
        }
    ]
}