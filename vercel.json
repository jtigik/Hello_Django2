{
    "builds": [
        {
            "src": "Hello_Django_2/HelloProject/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python3.11.7"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Hello_Django_2/HelloProject/wsgi.py"
        }
    ]
}